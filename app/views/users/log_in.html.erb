<div id="log-in-container" class="auth-form">
  <div class="links-container">
    <div class="sign-and-log-link unselected"><%= link_to "Sign Up", new_user_path %></div>
    <div class="sign-and-log-link selected">Log In</div>
  </div>
<%= form_tag '/sessions', method: :post do %>

  <br>
  <%= text_field_tag :username, nil, placeholder: "Username" %>
  <br>
  <%= password_field_tag :password, nil, placeholder: "Password" %>
  <br>
  <%= submit_tag "Log In" %>

<% end %>

</div>
